{
  "name": "webauthn-ui",
  "version": "0.2.5",
  "description": "WebAuthn helper module",
  "main": "dist/umd/webauthn-ui.js",
  "module": "dist/es/webauthn-ui.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build": "rollup -c rollup.config.js --environment NODE_ENV:development",
    "watch": "rollup -c rollup.config.js --environment NODE_ENV:development -w",
    "build:prod": "rollup -c rollup.config.js --environment NODE_ENV:production",
    "declaration": "tsc --declaration",
    "prepublishOnly": "npm run declaration && npm run build:prod"
  },
  "keywords": [
    "WebAuthn"
  ],
  "author": "Thomas Bleeker (http://www.madwizard.org)",
  "license": "MIT",
  "dependencies": {
    "@types/webappsec-credential-management": "^0.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^3.1.1",
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "rollup": "^2.26.5",
    "rollup-plugin-terser": "^7.0.0",
    "ts-jest": "^26.2.0",
    "tslib": "^2.0.1",
    "typescript": "3.9.*"
  }
}
